version: "2"
sql:
  - engine: "mysql"
    queries: "sql/queries/"
    schema: "sql/schema/schema.sql"
    gen:
      go:
        package: "models"
        out: "internal/models"
        emit_json_tags: true
        emit_prepared_queries: false
        emit_interface: true
        emit_exact_table_names: false
        emit_empty_slices: true